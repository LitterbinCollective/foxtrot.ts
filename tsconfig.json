{
  "compilerOptions": {
    "outDir": "dist/",
    "target": "es2021",
    "moduleResolution": "node",
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "sourceMap": true,
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["./*"],
      "@clu/*": ["./cluster/*"],
      "@man/*": ["./manager/*"],
      "@sha/*": ["./shared/*"]
    },
  },
}