    "dayjs": "^1.11.7",
    "detritus-client": "^0.16.3",
    "https-proxy-agent": "^5.0.1",
    "knex": "^2.4.0",
    "objection": "github:Vincit/objection.js",
    "pg": "^8.7.3",
    "pretty-ms": "^7.0.1",
    "sh": "github:LitterbinCollective/sh",
    "soundcloud-downloader": "github:LitterbinCollective/node-soundcloud-downloader",
    "sqlite3": "^5.0.11",
    "tweetnacl": "^1.0.3",
    "type-fest": "^2.1.0",
    "youtube-search-without-api-key": "^1.0.7",
